{
  "name": "agrocativo-backend",
  "version": "1.0.0",
  "description": "Servidor do projeto AgroCATIVO",
  "main": "agrocativo.js",
  "scripts": {
    "knex": "knex",
    "installdb": "knex migrate:rollback --all && yarn migrate && yarn seed",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "docs": "typedoc --out ./docs ./src",
    "tsc": "tsc",
    "build": "yarn tsc",
    "dev": "nodemon --watch 'src/' --exec ts-node src/agrocativo.ts -e ts",
    "prod": "pm2 stop agrocativo && yarn build && pm2 start ./dist/agrocativo.js",
    "start": "yarn build && node ./dist/agrocativo.js",
    "pretest": "knex migrate:rollback --all && yarn migrate && yarn seed",
    "test": "jest --coverage --no-cache --setupFiles dotenv/config"
  },
  "author": "Anast√°cio Loguercio",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-form-data": "^2.0.2",
    "@types/express-session": "^1.17.4",
    "@types/formidable": "^2.0.0",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.1",
    "@types/nodemailer": "^6.4.4",
    "@types/pdfmake": "^0.2.1",
    "@types/uuid": "^8.3.3",
    "nodemon": "^2.0.12",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.18",
    "express-session": "^1.17.2",
    "formidable": "^2.0.1",
    "jest": "^27.1.1",
    "jspdf": "^2.5.1",
    "knex": "^0.95.11",
    "multer": "^1.4.5-lts.1",
    "node-deeplink": "^0.6.0",
    "nodemailer": "^6.6.3",
    "pdfmake": "^0.2.5",
    "pg": "^8.7.1",
    "pm2": "^5.1.1",
    "sharp": "^0.30.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "uuid": "^8.3.2"
  }
}
